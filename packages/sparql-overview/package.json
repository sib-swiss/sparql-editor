{
  "name": "@sib-swiss/sparql-overview",
  "version": "0.0.2",
  "description": "A web component to easily deploy a user-friendly network visualization for one or more endpoints.",
  "license": "MIT",
  "author": {
    "name": "Vincent Emonet",
    "email": "vincent.emonet@gmail.com"
  },
  "type": "module",
  "main": "./dist/sparql-overview.umd.cjs",
  "module": "./dist/sparql-overview.js",
  "types": "./dist/sparql-overview.d.ts",
  "exports": {
    ".": {
      "import": {
        "development": "./src/sparql-overview.ts",
        "default": "./dist/sparql-overview.js"
      },
      "require": {
        "development": "./src/sparql-overview.ts",
        "default": "./dist/sparql-overview.umd.cjs"
      }
    }
  },
  "files": [
    "dist/*",
    "README.md",
    "../../LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "upgrade": "npx npm-check-updates -u && npm install",
    "version": "npm run build && npm publish"
  },
  "dependencies": {
    "@comunica/query-sparql": "^4.1.0",
    "@sigma/edge-curve": "^3.1.0",
    "@sigma/node-image": "^3.0.0",
    "graphology": "^0.26.0",
    "graphology-layout-force": "^0.2.4",
    "graphology-layout-forceatlas2": "^0.10.1",
    "iwanthue": "^2.0.0",
    "sigma": "^3.0.0"
  },
  "keywords": [
    "SPARQL",
    "Network visualization",
    "semantic web",
    "RDF",
    "Linked Data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sib-swiss/sparql-editor.git"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
